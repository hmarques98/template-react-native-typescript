    "react-dom": "17.0.1",
    "react-native-bundle-visualizer": "^2.2.1",
    "react-native-flipper": "^0.90.2",
    "react-test-renderer": "17.0.1",
    "redux-flipper": "^1.4.2",
    "regenerator-runtime": "^0.13.7",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint . --fix",
      "prettier --write .",
      "jest --bail --findRelatedTests"
    ]
  }
}
