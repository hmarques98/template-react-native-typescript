{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "run-android:stagingRelease": "ENVFILE=.env.staging react-native run-android --variant=release",
    "run-android:productionRelease": "ENVFILE=.env.prod react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "run-ios:stagingRelease": "react-native run-ios --scheme STAGING --configuration Release",
    "run-ios:productionRelease": "react-native run-ios --scheme PRODUCTION --configuration Release",
    "start": "react-native start --reset-cache",
    "test": "./jest.sh",
    "bundle": "react-native-bundle-visualizer",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "detox:ios": "./detox.sh ios",
    "detox:android": "./detox.sh android",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "generate": "plop",
    "validate-typescript": "tsc --noEmit",
    "uninstall-android": "adb uninstall com.helloworld",
    "clear-android": "adb shell pm clear com.helloworld",
    "uninstall-iOS": "xcrun simctl uninstall booted com.helloworld",
    "build-qa:android": "bundle exec fastlane android bump_badge_build_staging",
    "buildBundleProduction": "fastlane android bump_badge_build_production_bundle"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.4",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "@reduxjs/toolkit": "^1.5.1",
    "@sentry/react-native": "^2.4.3",
    "axios": "^0.21.1",
    "i18next": "^20.2.2",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "4.0.3",
    "react": "17.0.1",
    "react-i18next": "^11.8.15",
    "react-native": "0.64.1",
    "react-native-bootsplash": "^3.2.3",
    "react-native-code-push": "^7.0.0",
    "react-native-dotenv": "^2.5.0",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keychain": "^6.2.0",
    "react-native-onesignal": "^4.1.0",
    "react-native-paper": "^4.8.1",
    "react-native-permissions": "^3.0.1",
    "react-native-reanimated": "^2.3.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.18.1",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-user-avatar": "^1.0.7",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "11.4.1",
    "react-query": "^3.12.0",
    "react-redux": "^7.2.4",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-hooks": "^6.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/detox": "^17.14.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.170",
    "@types/react-native": "^0.64.0",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components-react-native": "^5.1.0",
    "@types/styled-system": "^5.1.10",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^18.6.2",
    "eslint": "^7.14.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-jest": "^24.1.5",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-styled-components": "^7.0.4",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.5.3",
